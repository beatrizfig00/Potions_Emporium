+package Negocio;
+
+import java.util.List;
+
+public class Caixa {
+    private double totalCompra;
+    private double totalComDesconto;
+    private Promocao promocao;
+    private Cliente cliente;
+
+    public Caixa(Promocao promocao, Cliente cliente) {
+        this.promocao = promocao;
+        this.cliente = cliente;
+    }
+
+    public void calcularTotal(Pedido pedido) {
+        this.totalCompra = pedido.getTotal();
+    }
+
+    public void aplicarDesconto(Pedido pedido) {
+        if (promocao != null && promocao.validarPromocao()) {
+            this.totalComDesconto = promocao.aplicarPromocao(pedido, cliente);
+        } else {
+            this.totalComDesconto = totalCompra;
+        }
+    }
+
+    public boolean processarPagamento(Pagamento pagamento, Pedido pedido) {
+        calcularTotal(pedido);
+        aplicarDesconto(pedido);
+
+        int totalCompraNuques = Moeda.galeoesParaNuques((int) totalComDesconto);
+
+        int galeoes = pagamento.getValor();
+        int sicles = pagamento.getValor();
+        int nuques = pagamento.getValor();
+
+        int totalPagoNuques = Moeda.galeoesParaNuques(galeoes)
+                + Moeda.siclesParaNuques(sicles)
+                + nuques;
+
+        // Processa o pagamento e verifica se o valor pago é suficiente
+        return pagamento.processarPagamento(
+                galeoes,
+                sicles,
+                nuques
+        );
+    }
+}
